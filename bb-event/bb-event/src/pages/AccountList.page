<apex:page standardController="Account" recordSetVar="accounts">
    <apex:pageBlock title="List of Accounts">
        <ul>
       	<apex:repeat value="{! accounts}" var="a">
            <li><apex:outputLink value="/{! a.Id }">{! a.Name}</apex:outputLink></li>
 		</apex:repeat>
        </ul>>
    </apex:pageBlock>
</apex:page>